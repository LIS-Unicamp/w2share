<form class="navbar-form navbar-left" action="{{ path("provenance-query") }}" method="GET">
    <div class="form-group">
        <input name="query" value="{{ query }}" type="text" class="form-control" placeholder="Search">
    </div>
    
    <div class="form-group">
        <select name="concept" class="form-control">
            {% for row in result %}
                <option value="{{ row.concept }}" {% if row.concept == concept %}selected{% endif %}>{{ row.concept }}</option>
            {% endfor %}
        </select>
    </div>
    <button type="submit" class="btn btn-default">Submit</button>
</form>
