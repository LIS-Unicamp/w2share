{% extends 'base.html.twig' %}
{% block title %}Workflows Run{% endblock %}
{% block body %}
<h1>Workflows Run</h1>
<table class="table-bordered">
    <thead>
        <tr>
            <th>Workflow Run</th>
            <th>Workflow</th>
            <th>Start Time</th>
            <th>End Time</th>
        </tr>
    </thead>
    <tbody>
{% for row in result %}
    <tr>
        <td><a href="{{ path('provenance-workflow-run', { 'workflow_run': row.workflowRun|url_encode }) }}">{{ row.workflowRun}}</a></td>
        <td><a href="{{ path('provenance-workflow', { 'workflow': row.workflow|url_encode }) }}">{{ row.label }}</a></td>
        <td>{{ row.startedAtTime }}</a></td>
        <td>{{ row.endedAtTime }}</td>
    </tr>
{% endfor %}
    </tbody>
</table>
{% endblock %}  
{% block javascripts %}            
{% endblock %}
{% block stylesheets %}
{% endblock %} 
