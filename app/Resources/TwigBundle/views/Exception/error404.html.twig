{% extends 'base.html.twig' %}
{% block title %}Page or resource not found - Error 404{% endblock %}
{% block body %}
    <h1>Error 404: Not found</h1>

    <center class="background error-page-wrapper">
        <div class="content-container shadow">
          <div class="head-line secondary-text-color">
            404
          </div>
          <div class="subheader primary-text-color">
            Oops, the page or resource you're looking for does not exist.
          </div>
          <div class="hr"></div>
          <div class="context secondary-text-color">
            <p>
              Checkout for any URL misspelling. 
              You may want to head back to the homepage.
              If you think something is broken, report a problem.
            </p>
          </div>
          <div class="buttons-container">
            <a class="button" href="{{ path("homepage") }}"><span class="fa fa-home"></span> Go To Homepage</a>
            <a class="button" href="{{ path("contact") }}"><span class="fa fa-warning"></span> Report A Problem</a>
          </div>
        </div>
    </center>    
{% endblock %}
{% block javascripts %} 
{% endblock %}
{% block stylesheets %}
<link rel="stylesheet" href="{{ asset("css/error404Page.css") }}" />
{% endblock %} 